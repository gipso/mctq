sjl(msw, msf, abs = FALSE, method = "difference")
#> [1] "5400s (~1.5 hours)" # Expected
sjl(msw, msf, abs = FALSE, method = "shortest") # default method
#> [1] "5400s (~1.5 hours)" # Expected
